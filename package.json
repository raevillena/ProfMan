{
  "name": "profman",
  "version": "1.0.0",
  "description": "Full-stack professor management system with Google Drive integration",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:watch": "concurrently \"npm run dev:backend:watch\" \"npm run dev:frontend:watch\"",
    "dev:backend:watch": "cd backend && npm run dev:watch",
    "dev:frontend:watch": "cd frontend && npm run dev:watch",
    "build": "npm run build:backend && npm run build:frontend",
    "build:dev": "npm run build:backend && npm run build:frontend:dev",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "build:frontend:dev": "cd frontend && npm run build:dev",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd backend && npm run start",
    "start:frontend": "cd frontend && npm run preview:prod",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "cd backend && npm test",
    "test:frontend": "cd frontend && npm test",
    "lint": "npm run lint:backend && npm run lint:frontend",
    "lint:backend": "cd backend && npm run lint",
    "lint:frontend": "cd frontend && npm run lint",
    "seed": "cd backend && npm run seed",
    "seed:all": "cd backend && npm run seed:all",
    "seed:users": "cd backend && npm run seed:users",
    "seed:subjects": "cd backend && npm run seed:subjects",
    "seed:branches": "cd backend && npm run seed:branches",
    "seed:quizzes": "cd backend && npm run seed:quizzes",
    "seed:announcements": "cd backend && npm run seed:announcements",
    "seed:data": "cd backend && npm run seed:data",
    "seed:test": "cd backend && npm run seed:test",
    "test:firebase": "cd backend && npm run test:firebase",
    "test:drive": "cd backend && npm run test:drive"
  },
  "keywords": ["education", "professor", "management", "firebase", "react"],
  "author": "ProfMan Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
