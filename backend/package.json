{
  "name": "profman-backend",
  "version": "1.0.0",
  "description": "Backend API for ProfMan - Professor Management System",
  "main": "dist/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node src/index.ts",
    "dev:watch": "cross-env NODE_ENV=development ts-node --watch src/index.ts",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "start:dev": "cross-env NODE_ENV=development node dist/index.js",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "seed": "cross-env NODE_ENV=development ts-node src/scripts/seed.ts",
    "seed:all": "cross-env NODE_ENV=development ts-node src/scripts/seedAll.ts",
    "seed:users": "cross-env NODE_ENV=development ts-node src/scripts/seedUsers.ts",
    "seed:subjects": "cross-env NODE_ENV=development ts-node src/scripts/seedSubjects.ts",
    "seed:branches": "cross-env NODE_ENV=development ts-node src/scripts/seedBranches.ts",
    "seed:quizzes": "cross-env NODE_ENV=development ts-node src/scripts/seedQuizzes.ts",
    "seed:announcements": "cross-env NODE_ENV=development ts-node src/scripts/seedAnnouncements.ts",
    "seed:data": "cross-env NODE_ENV=development ts-node src/scripts/seedData.ts",
    "seed:test": "cross-env NODE_ENV=development ts-node src/scripts/seedTest.ts",
    "create:professor": "cross-env NODE_ENV=development ts-node src/scripts/createProfessor.ts",
    "create:student": "cross-env NODE_ENV=development ts-node src/scripts/createStudent.ts",
    "test:firebase": "cross-env NODE_ENV=development ts-node src/scripts/testFirebase.ts",
    "test:drive": "cross-env NODE_ENV=development ts-node src/scripts/testGoogleDrive.ts"
  },
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "firebase-admin": "^12.0.0",
    "googleapis": "^128.0.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "cross-env": "^10.0.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "keywords": [
    "education",
    "professor",
    "management",
    "api",
    "firebase"
  ],
  "author": "ProfMan Team",
  "license": "MIT"
}
